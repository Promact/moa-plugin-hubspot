{
  "name": "@promactinfo/moa-plugin-hubspot",
  "description": "Hubspot provider for moa cli",
  "version": "0.1.1",
  "author": "Chintan Shah",
  "bin": {
    "hubspot": "./bin/run.js"
  },
  "bugs": "https://github.com/Promact/moa-plugin-hubspot/issues",
  "dependencies": {
    "@hubspot/api-client": "^11.0.0",
    "@inquirer/prompts": "^8.2.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@promactinfo/moa-cli": "^0.1.0",
    "cli-table3": "^0.6.5"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.13",
    "@eslint/compat": "^1",
    "@eslint/js": "^9.39.2",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "eslint-config-xo": "^0.49.0",
    "eslint-config-xo-space": "^0.35.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^62.5.0",
    "eslint-plugin-mocha": "^11.2.0",
    "eslint-plugin-n": "^17.23.2",
    "eslint-plugin-perfectionist": "^5.4.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5",
    "typescript-eslint": "^8.54.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/Promact/moa-plugin-hubspot",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "hubspot",
    "dirname": "hubspot",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "repository": "Promact/moa-plugin-hubspot",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "lint:ci": "biome check .",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}